{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs-users/company-about",
  "version": "1.0.0",
  "title": "Company About",
  "category": "frs-blocks",
  "icon": "welcome-learn-more",
  "description": "Display company biography and description from user profile",
  "keywords": [
    "company",
    "about",
    "biography",
    "description"
  ],
  "textdomain": "frs-users",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "attributes": {
    "user_id": {
      "type": "number",
      "default": 0
    },
    "showHeadshot": {
      "type": "boolean",
      "default": true
    },
    "layout": {
      "type": "string",
      "default": "sidebyside"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "example": {}
}