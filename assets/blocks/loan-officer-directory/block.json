{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs/lo-directory",
  "version": "1.0.0",
  "title": "Loan Officer Directory",
  "category": "frs-blocks",
  "icon": "businessperson",
  "description": "Display a searchable directory of loan officers with hero, sidebar filters, state chips, and QR modal.",
  "keywords": [
    "directory",
    "loan officer",
    "team",
    "mortgage"
  ],
  "textdomain": "frs-users",
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "attributes": {
    "showHero": {
      "type": "boolean",
      "default": true
    },
    "perPage": {
      "type": "number",
      "default": 12
    },
    "columns": {
      "type": "number",
      "default": 4
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}