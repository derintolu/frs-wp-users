{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs/lo-directory",
  "version": "1.0.0",
  "title": "Loan Officer Directory",
  "category": "frs-blocks",
  "icon": "businessperson",
  "description": "Searchable loan officer directory with customizable hero section. Add any blocks to the hero area.",
  "keywords": [
    "directory",
    "loan officer",
    "team",
    "mortgage"
  ],
  "textdomain": "frs-users",
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "attributes": {
    "perPage": {
      "type": "number",
      "default": 12
    },
    "columns": {
      "type": "number",
      "default": 4
    },
    "searchPlaceholder": {
      "type": "string",
      "default": "Search by name, city, or state..."
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style.css",
  "render": "file:./render.php"
}
