{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs/lo-grid",
  "version": "1.0.0",
  "title": "Loan Officer Grid",
  "category": "frs-blocks",
  "icon": "grid-view",
  "description": "Searchable loan officer directory grid with sidebar filters and state chips. Design your own hero above this block.",
  "keywords": [
    "directory",
    "loan officer",
    "grid",
    "team",
    "mortgage"
  ],
  "textdomain": "frs-users",
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "attributes": {
    "perPage": {
      "type": "number",
      "default": 12
    },
    "columns": {
      "type": "number",
      "default": 4
    },
    "showSidebar": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
