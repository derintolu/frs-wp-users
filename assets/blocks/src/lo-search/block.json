{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs/lo-search",
  "version": "1.0.0",
  "title": "LO Directory Search",
  "category": "frs-blocks",
  "icon": "search",
  "description": "Search bar that filters the Loan Officer Grid. Place in your hero section above the grid.",
  "keywords": [
    "search",
    "filter",
    "directory",
    "loan officer"
  ],
  "textdomain": "frs-users",
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "placeholder": {
      "type": "string",
      "default": "Search by name, city, or state..."
    },
    "buttonText": {
      "type": "string",
      "default": "Search"
    },
    "showButton": {
      "type": "boolean",
      "default": true
    },
    "size": {
      "type": "string",
      "default": "large"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style.css",
  "render": "file:./render.php"
}
