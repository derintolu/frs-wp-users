{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frs-users/loan-officer-directory",
  "version": "1.0.0",
  "title": "Loan Officer Directory",
  "category": "frs-blocks",
  "icon": "businessperson",
  "description": "Filterable directory of loan officers with state selection",
  "keywords": [
    "directory",
    "loan officer",
    "team",
    "filter"
  ],
  "textdomain": "frs-users",
  "render": "file:./render.php",
  "attributes": {
    "showFilters": {
      "type": "boolean",
      "default": true
    },
    "postsPerPage": {
      "type": "number",
      "default": 12
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "example": {}
}